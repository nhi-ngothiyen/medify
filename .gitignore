cat > .gitignore <<'GITIGNORE'
# ===== OS / IDE =====
.DS_Store
.idea/
.vscode/

# ===== Secrets / Env =====
.env
.env.*
*.key
*.pem

# ===== Python backend: medify-be =====
medify-be/.venv/
medify-be/venv/
medify-be/__pycache__/
medify-be/**/*.pyc
medify-be/**/*.pyo
medify-be/**/*.pyd
medify-be/**/*.egg-info/
medify-be/.pytest_cache/
medify-be/.mypy_cache/
medify-be/.ruff_cache/
medify-be/.coverage
medify-be/htmlcov/

# Alembic caches
medify-be/alembic/versions/__pycache__/

# ===== Admin web (Node/TS): medify-admin =====
medify-admin/node_modules/
medify-admin/.next/
medify-admin/.nuxt/
medify-admin/dist/
medify-admin/build/
medify-admin/.turbo/
medify-admin/.eslintcache

# ===== Flutter app: medify_app =====
medify_app/.dart_tool/
medify_app/.packages
medify_app/.flutter-plugins
medify_app/.flutter-plugins-dependencies
medify_app/pubspec.lock
medify_app/build/
# Android subfolder
medify_app/android/.gradle/
medify_app/android/local.properties
medify_app/android/app/build/
# iOS subfolder
medify_app/ios/Pods/
medify_app/ios/Runner.xcworkspace/
medify_app/ios/Flutter/Flutter.framework
medify_app/ios/Flutter/Flutter.podspec
GITIGNORE
